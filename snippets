<hr>

<button onclick="add()">+ Add</button>
<h1 id="demo"></h1>
<button onclick="less()">- less</button>
<script>
var item = 0;
document.getElementById('demo').innerHTML = 0;

function add() {

	document.getElementById('demo').innerHTML = item++;
}
function less() {
	document.getElementById('demo').innerHTML = item--;
}
</script>

<html>
<script>
function pageloadHandler() {
	var myimage =  document.getElementById('mypic');
	myimage.onclick = function() {
		alert('Clicked on image...')
	}
}

window.onload = function() {
	pageloadHandler();
}
</script>
<body>
<img src="https://www.google.com/logos/doodles/2019/spring-equinox-2019-northern-hemisphere-5139135894388736.2-s.png" id="mypic">
</body>
</html>


*************************
DYNAMIC BOX ADD ON Click
*************************
<style>
.box {
width:120px;
height:120px;
background:#efefef;
border:solid 1px #ccc;
display:inline-block;
margin:10px;
}
</style>
<button onclick="addNew()">add new</button><br>
<div id='demo' class='box'></div>

<script>
function addNew() {
	var div = document.createElement('div');
	div.className = 'box';
	document.body.appendChild(div);
	console.log('DIV---:', div)
}

// ON CLICK ADD BOX DYNAMICALLY

*************************
DYNAMIC INPUT FIELDS ADD ADD AND REMOVE
*************************

<div id='content'></div>
<button onclick='addRow()'>Add New</button>
<script>
function addRow() {
    var div = document.createElement('div');
    div.innerHTML =
        '<input type="text" name="name" value="" />\
        <input type="text" name="value" value="" />\
        <label> <input type="checkbox" name="check" value="1" /> Checked? </label>\
        <input type="button" value="-" onclick="removeRow(this)">';
    document.getElementById('content').appendChild(div);
}
function removeRow(input) {
    document.getElementById('content').removeChild(input.parentNode);
}

// MULTI INPUTS ADD ON CLICK

*************************
DIV HIDE SHOW
*************************
<button onclick='myfun()'>Show/Hide</button>
<h1 id='demo'>Heading one</h1>

<script>
var x = document.getElementById('demo');
function myfun() {
	if(x.style.display === 'none') {
		x.style.display = 'block'
	} else {
		x.style.display = 'none'
	}
}

// display block
// display none

*************************
LIKE DISLIKE 
*************************

<div class='one' onclick='myfun(this)'></div>
<style>
.one {background:red; width:50px; height:50px;}
.two {background:green; width:50px; height:50px;}
</style>


function myfun(x) {
	x.classList.toggle('two');
}

// oncilck change the bg/ like - dislike

*************************
TOGGLE TEXT
*************************
<button onclick='myfun(this)'>Toggle text</button>
<h1 id='demo'></h1>

function myfun() {
	var x = document.getElementById('demo');
	
	if(x.innerHTML === 'Hello') {
		x.innerHTML = 'World';
	} else {
		x.innerHTML = 'Hello';
	}
}

// Hello
// World

*************************
ONCLICK CHANGE CLASS
*************************
<button onclick='myfun()'>Click here</button>
<div class='one' id='demo'>This is dynamic div</div>
<style>
.one {background:transparent; width:300px; height:60; text-align:center; line-height:60px;}
.two {background:red; width:300px; height:60; text-align:center}

</style>
<script>
function myfun() {
var x = document.getElementById('demo')
	x.classList.toggle('two');
}

// This is dynamic div
