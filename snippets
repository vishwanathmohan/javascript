<p id='demo'></p>
<button onclick='myfun()'>Click me</button>

var colors = ['orange','green','yellow','blue'];

document.getElementById('demo').innerHTML = colors;

function myfun() {
	document.getElementById('demo').innerHTML = colors.copyWithin(2,0)
}
// orange,green,yellow,blue

// orange,green,orange,green
