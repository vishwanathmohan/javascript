var a = 10;
var b = 20;
var c = 'test';
var spacer = '<br>'
var main = '<h1>'
var mainClose = '</h1>'

resultAddition = a + b
resultSubstraction = a - b
resultMultiple = a * b
resultDivide = a / b
resultModules = a % b

document.write(main)
document.write('resultAddition :'+ resultAddition)
document.write(spacer)
document.write('resultSubstraction :', resultSubstraction)
document.write(spacer)
document.write('resultMultiple :', resultMultiple)
document.write(spacer)
document.write('resultDivide :', resultDivide)
document.write(spacer)
document.write('resultModules :', resultModules)
document.write(mainClose)
