<pre>

<h1 id="demo"></h1>
<button onclick="myfunction()">Submit</button>

var ages = [16, 17,20, 28, 32];

function checkage(ages) {
	return ages >= 20;
}

function myfunction() {
	document.getElementById('demo').innerHTML = ages.filter(checkage);
}

// 20,28,32

