*************************
REPLACE THE TEXT
*************************
<button onclick='myfun()'>Click me</button>
<h1 id='demo'>Welcome to Microsoft</h1>

<script>
function myfun() {
	var x = document.getElementById('demo').innerHTML;
	var txt = x.replace('Microsoft','Google');
	document.write('<h1>'+txt+'</h1>')
}
// Google

*************************
ONCLICK LOWER TO UPPER CASE
*************************
<p id='demo'>hello world</p>
<button onclick='myfun()'>Click here</button>
<script>
function myfun() {
	var x = document.getElementById('demo').innerHTML;
	result = x.toUpperCase();
	document.write(result);
}

// HELLO WORLD

*************************
MERGE TWO ARRAYS WITH MAP
*************************

var a1 = ["a", "b", "c"];
var a2 = [1, 2, 3];
var a3 = a1.map(function(e,i)
{ 
	return [e,a2[i] + '<br>'] 
}
);
document.write(a3)
console.log(a3)

*************************
GET THE DATE IN ARRAY
*************************

var myarr = [ 
	  [ 10, '1MUa4qAoC3cIO703ccomvo' ],
      [ 10, '3RknqHDaBlIyn38U1s77nD' ],
      [ 10, '6xS8xyE7sFhnPveLERldR3' ]
]

for(var i=0; i < myarr.length; i++) {	
	document.write(myarr[i], '<br>')
}

// 10,1MUa4qAoC3cIO703ccomvo
10,3RknqHDaBlIyn38U1s77nD
10,6xS8xyE7sFhnPveLERldR3

*************************
GET MAX & MIN NUMBERS IN ARRAY
*************************

<h1 id='demo'></h1>
var numbers = [3,6,8,1,2,5]

max = Math.max.apply(null, numbers);
min = Math.min.apply(null, numbers);

document.getElementById('demo').innerHTML = 'Max Number : ' + max + 
'<br>'
 + 'Min Number : ' + min;

// MAX NUMBER : 8
// MIN NUMBER : 1

*************************
GET KEYS FROM OBJECT
*************************
var c = {
	n: 'a',
	m: 'b',
	o: 'c',
	p: 'd',
	q: 'e'
}

document.write(Object.keys(c))

// n, m, o, p, q

*************************
GET VALUES FROM OBJECT
*************************

var c = {
	n: 'a',
	m: 'b',
	o: 'c',
	p: 'd',
	q: 'e'
}

document.write(Object.values(c))

// a, b, c, d, e

*************************
GET LOOP FROM OBJECT
*************************
var c = {
	a : 'g',
	b : 'h',
	c : 'i',
	d : 'j',
	e : 'k',
	f : 'l'
}

document.write(Object.entries);
*************************
GET VALUES FROM OBJECT KEY VALUE WITH NUMBERS
*************************
var p = {
    40: 'a',
	90: 'b',
	20: 'c',
	10: 'd'
};
document.write(Object.values(p))

// d, c, a, b
note : It will show assending order of key value

*************************
TO FIND MAX NUMBER
*************************
var num = [-1, -13, -30, 3999,0, 3, 4, 5, 89, 34, 67, 899];
result = Math.max.apply(null, num);
document.write(result)

// 3999

*************************
GET MAX NUMBER IN ARRAY
*************************
var num = [2,5,67,89,12,45,78,90];
result = Math.max.apply(null, num);
document.write(result);

// 90

GET MAX NUMBERS

result = Math.max(5,7,8,56, 345)
document.write(result);

// 345

*************************
GET OBJECT KEYS AND VALUES USING ENTRIES
*************************
var c = {
	a : 'g',
	b : 'h',
	c : 'i',
	d : 'j',
	e : 'k',
	f : 'l'
}

result = Object.entries(c)
console.log(result)
document.write(result);

//console :
0: (2) ["a", "g"]
1: (2) ["b", "h"]
2: (2) ["c", "i"]
3: (2) ["d", "j"]
4: (2) ["e", "k"]
5: (2) ["f", "l"]

//browser :
a,g,b,h,c,i,d,j,e,k,f,l

*************************
GET OBJECT VALUES USING LOOP
*************************
const c = {
	a : 'gggg',
	b : 'hhhh',
	c : 'iiii',
	d : 'jjjj',
	e : 'kkkk',
	f : 'llll'
}
const values = Object.values(c)
for(const value of values) {
	document.write(value + '<br>');
}

//
gggg
hhhh
iiii
jjjj
kkkk
llll
*************************
SAMPLE IF CONDITION
*************************
var item = 'ice cream';

if(item == 'ice cream') {
	document.write("Yey you are right!!");
} else {
	document.write("Oops...");
}

// Yey you are right!!


